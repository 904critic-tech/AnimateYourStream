{
  "name": "mixamo-model-viewer",
  "version": "1.0.0",
  "description": "Advanced 3D Model Viewer with AI-powered features, lip sync, and Mixamo-style interface",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "build:production": "npm run type-check && npm run lint && npm run build",
    "preview": "vite preview",
    "preview:production": "vite preview --host 0.0.0.0 --port 4173",
    "test": "jest",
    "test:e2e": "cypress open",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build:analyze && npx vite-bundle-analyzer dist/stats.html",
    "size-check": "npm run build && du -sh dist/*",
    "ingest:blender": "pwsh -File scripts/blender_ingest.ps1 -ScriptPath \"Default_Characters/CRZ_9_Fortnite/your_script.py\" -CharacterName \"CRZ_9_Fortnite\" -GlbName \"cyberpunk_idle.glb\" -GltfName \"cyberpunk_idle.gltf\" -BlendName \"cyberpunk_idle.blend\"",
    "manifest:characters": "node scripts/generate_character_manifest.cjs",
    "post-ingest": "npm run manifest:characters",
    "builder:serve": "node scripts/server/build_service.cjs",
    "license:init": "node scripts/licenses/init_keys.cjs",
    "license:gen": "node scripts/licenses/generate_code.cjs",
    "license:gen:7": "pwsh -File scripts/licenses/Generate-7day-Code.ps1",
    "license:gen:30": "pwsh -File scripts/licenses/Generate-30day-Code.ps1",
    "agent:watch:lint": "eslint . --ext ts,tsx --cache --cache-location .tmp/eslint | cat",
    "agent:watch:typecheck": "tsc --noEmit --watch",
    "agent:monitor:builder": "node scripts/server/ping_builder_health.cjs",
    "agents:start": "concurrently -n lint,typecheck,builder -c yellow,cyan,magenta \"npm:agent:watch:lint\" \"npm:agent:watch:typecheck\" \"npm:agent:monitor:builder\"",
    "vercel-build": "vite build",
    "image3d:mvp": "node scripts/image_to_3d/single_image_mvp.cjs --input public/favicon.ico --slug sample",
    "test:image3d:mvp": "node scripts/test_image_to_3d_mvp.cjs"
  },
  "bin": {},
  "scriptsExt": {},
  "scripts2": {},
  "builder": {},
  "dependencies": {
    "@react-three/drei": "^9.88.13",
    "@react-three/fiber": "^8.15.11",
    "@types/three": "^0.157.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "lucide-react": "^0.294.0",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.1.2",
    "tailwindcss": "^3.3.5",
    "three": "^0.157.0",
    "three-stdlib": "^2.27.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "localtunnel": "^2.0.2",
    "postcss": "^8.4.31",
    "puppeteer": "^24.16.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "terser": "^5.43.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.17.0"
  },
  "keywords": [
    "3d",
    "model-viewer",
    "mixamo",
    "three.js",
    "lip-sync",
    "ai",
    "animation"
  ],
  "author": "Animation Studio Team",
  "license": "MIT"
}
